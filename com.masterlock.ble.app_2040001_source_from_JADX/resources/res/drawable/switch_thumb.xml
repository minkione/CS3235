<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <item android:left="@dimen/switch_compat_thumb_margin" android:top="@dimen/switch_compat_thumb_margin" android:right="@dimen/switch_compat_thumb_margin" android:bottom="@dimen/switch_compat_thumb_margin">
        <selector>
            <item android:state_checked="true">
                <shape android:shape="oval">
                    <size android:height="@dimen/switch_compat_thumb_size" android:width="@dimen/switch_compat_thumb_size"/>
                    <solid android:color="@color/white"/>
                </shape>
            </item>
            <item>
                <shape android:shape="oval">
                    <size android:height="@dimen/switch_compat_thumb_size" android:width="@dimen/switch_compat_thumb_size"/>
                    <stroke android:width="@dimen/switch_compat_thumb_stroke_width" android:color="@color/white"/>
                    <solid android:color="@color/white"/>
                </shape>
            </item>
        </selector>
    </item>
</layer-list>
