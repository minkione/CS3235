        <item name="android:orientation">vertical</item>
        <item name="android:background">@drawable/basepopu_bg</item>
        <item name="android:layout_width">300dp</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="myPopu_title">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/myblack</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">left|center_horizontal|clip_horizontal</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="myPopu_line">
        <item name="android:background">@color/myline</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1px</item>
    </style>
    <style name="myline_w">
        <item name="android:background">@color/myline</item>
