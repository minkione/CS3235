    <style name="myMenuItem_title">
        <item name="android:textColor">@color/mywhite</item>
        <item name="android:gravity">left|center_horizontal|clip_horizontal</item>
        <item name="android:layout_height">50dp</item>
    </style>
    <style name="myMenuItem">
        <item name="android:textColor">@color/mywhite</item>
        <item name="android:gravity">left|center_horizontal|clip_horizontal</item>
        <item name="android:layout_height">50dp</item>
    </style>
    <style name="myMenuItem_line">
        <item name="android:background">@color/menu_line_bg</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
    </style>
    <style name="myTitle">
        <item name="android:background">@color/title_bg</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
    </style>
    <style name="myTitle_line">
        <item name="android:background">@color/title_line</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
    </style>
    <style name="PLTitle">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">left|center_horizontal|clip_horizontal</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="PLTitleButton">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">left|center_horizontal|clip_horizontal</item>
        <item name="android:visibility">gone</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>
    <style name="PLTitleButtonBack">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>
    <style name="myGeneralItem_line">
        <item name="android:background">@color/general_line_bg</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
    </style>
    <style name="MyEditText">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/myblack</item>
        <item name="android:textColorHint">@color/mygray</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">left|center_horizontal|clip_horizontal</item>
        <item name="android:background">@color/transparent</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:singleLine">true</item>
    </style>
